#include<stdio.h>
int main(){
    int n=0,i=0,x=0; 
    scanf("%d",&n);                //先输入n判断接下来要输入几行数据。
    int sum[20]={0};
    for(i=0;i<n;i++)
    {
        for(sum[i]=0,x=0;x!=-1;)   //当输入-1结束该行数据（只有一个-1）。
        {
            scanf("%d",&x);
            if(x%3==1&&x%5==2)     //如果该数%3为1且%5为2，则求和。
            {
                sum[i]+=x;
            }
        }
    }
    for(i=0;i<n;i++)
    {
        printf("%d\n",sum[i]);
    }
    return 0;
}
