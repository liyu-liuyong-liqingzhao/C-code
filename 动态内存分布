#include<stdio.h>
#include<stdlib.h>
void getmemory(char*p)
{
    p=(char*)malloc(10);
    return p;         //将p返回就可以改变str的内容。
}
int main(){
    char* str=NULL;
    getmemory(str);   //传递形参，只是把str的内容复制给p，str仍是NULL。此处改成&str也可以。
    printf(str);      //输出格式没有错误。
    free(str);
    str=NULL;
    return 0;
    }
